{
  "name": "@floralink/web",
  "private": true,
  "type": "module",
  "version": "3.3.2",
  "description": "Web application for linking and analyzing taxon specific data with plant occurrence data",
  "author": "Vincent Ruge",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt dev -o",
    "host": "nuxt dev --host -o",
    "postinstall": "nuxt prepare",
    "generate": "node --max-old-space-size=4096 ./node_modules/nuxt/bin/nuxt.mjs generate",
    "serve": "serve .output/public",
    "deps": "npm-check-updates --interactive --format group",
    "release": "changelogen --noAuthors --release"
  },
  "dependencies": {
    "@floralink/core": "^0.1.3",
    "@floralink/databases": "^0.1.7",
    "@floralink/plugins": "^0.2.2",
    "@iconify-json/heroicons": "^1.2.3",
    "@nuxt/fonts": "^0.14.0",
    "@nuxt/ui": "^2.22.3",
    "@nuxtjs/seo": "3.4.0",
    "@pinia/nuxt": "^0.11.3",
    "@vueuse/nuxt": "^14.1.0",
    "nuxt": "^4.3.0",
    "nuxt-security": "^2.5.1",
    "object-hash": "^3.0.0",
    "pinia": "^3.0.4",
    "plotly.js-dist": "3.3.1",
    "plotly.js-locales": "3.3.1",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "yup": "1.7.1"
  },
  "devDependencies": {
    "@types/object-hash": "^3.0.6",
    "changelogen": "^0.6.2",
    "npm-check-updates": "^19.3.1"
  }
}
