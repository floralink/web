{
  "name": "@floralink/web",
  "private": true,
  "type": "module",
  "version": "3.2.0",
  "description": "Web application for linking and analyzing taxon specific data with plant occurrence data",
  "author": "Vincent Ruge",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt dev -o",
    "host": "nuxt dev --host -o",
    "postinstall": "nuxt prepare",
    "generate": "node --max-old-space-size=4096 ./node_modules/nuxt/bin/nuxt.mjs generate",
    "serve": "serve .output/public",
    "deps:check": "npm-check-updates --interactive --format group && npm audit fix npm run postinstall",
    "deps:update": "npm-check-updates --format group --target minor -u --install always && npm audit fix && npm run postinstall",
    "release": "changelogen --noAuthors --release --no-tag"
  },
  "dependencies": {
    "@floralink/core": "^0.1.3",
    "@floralink/databases": "^0.1.7",
    "@floralink/plugins": "^0.2.2",
    "@iconify-json/heroicons": "^1.2.3",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/ui": "^2.22.3",
    "@nuxtjs/seo": "3.2.2",
    "@pinia/nuxt": "^0.11.2",
    "@vueuse/nuxt": "^13.9.0",
    "nuxt": "^3.19.2",
    "nuxt-security": "^2.4.0",
    "object-hash": "^3.0.0",
    "pinia": "^3.0.3",
    "plotly.js-dist": "3.1.0",
    "plotly.js-locales": "3.1.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "yup": "1.7.1"
  },
  "devDependencies": {
    "@types/object-hash": "^3.0.6",
    "vite": "^6.3.6"
  }
}
